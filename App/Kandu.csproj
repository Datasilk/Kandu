    <StartupObject>Datasilk.Program</StartupObject>
  </PropertyGroup>

  <ItemGroup>
    <Content Remove="Core\config.json" />
  </ItemGroup>

  <ItemGroup>
    <None Remove="Pages\404.html" />
    <None Remove="Pages\500.html" />
    <None Remove="Pages\Boards\boards.html" />
    <None Remove="Pages\Boards\list-item.html" />
    <None Remove="Pages\Board\board.html" />
